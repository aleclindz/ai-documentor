{
  "name": "ai-documentor",
  "version": "1.0.0",
  "description": "AI-powered documentation generator that creates comprehensive docs for any codebase using OpenAI GPT-4",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "ai-documentor": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/templates dist/",
    "dev": "ts-node src/cli.ts",
    "start": "node dist/cli.js",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "keywords": [
    "documentation",
    "ai",
    "openai",
    "gpt-4",
    "code-analysis",
    "automatic-documentation",
    "developer-tools",
    "cli",
    "mermaid",
    "markdown",
    "typescript",
    "javascript"
  ],
  "author": "Alec Lindsay <aleclindsay@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleclindz/ai-documentor.git"
  },
  "bugs": {
    "url": "https://github.com/aleclindz/ai-documentor/issues"
  },
  "homepage": "https://github.com/aleclindz/ai-documentor#readme",
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/preset-env": "^7.28.0",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "dotenv": "^17.2.1",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "marked": "^11.1.1",
    "mermaid": "^10.6.1",
    "openai": "^4.104.0",
    "ora": "^8.0.1",
    "ts-morph": "^21.0.1",
    "typescript": "^5.3.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
